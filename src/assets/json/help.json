                "Auto Remind": {
                    "usage": ",autoremind <time/off>",
                    "short": "Auto ping X minutes before the pound opens",
                    "description": "Allows you to set an Auto Remind to ping you at a time you set before the pound opens. The furthest you can set an Auto Remind for at the moment is 60 minutes.",
                    "examples": {
                        "1": ",autoremind 5m",
                        "2": ",autoremind 60m",
                        "3": ",autoremind off"
                    },
                    "aliases": {}
                },

                "Remind Me": {
                    "usage": ",remindme <time>",
                    "short": "Pings you after specified amount of time",
                    "description": "Pings you after a specified amount of time that you set. The maximum reminding time allowed is 24 hours.",
                    "examples": {
                        "1": ",remindme 1h6m23s",
                        "2": ",remindme 12m",
                        "3": ",remindme 1h10s"
                    },
                    "aliases": {
